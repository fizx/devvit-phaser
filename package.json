{
  "name": "devvit-phaser",
  "version": "0.1.0",
  "description": "A library to integrate Phaser.js with Reddit's Devvit platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "docs": "typedoc && cp README.md docs/ && echo '<!DOCTYPE html><html><head><meta http-equiv=\"refresh\" content=\"0; url=api/index.html\"></head><body>Redirecting to <a href=\"api/index.html\">documentation</a>...</body></html>' > docs/index.html",
    "docs:serve": "npx serve docs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "deploy-docs": "npm run build && npm run docs && gh-pages -d docs"
  },
  "keywords": [
    "devvit",
    "phaser",
    "reddit",
    "games"
  ],
  "author": "Kyle Dawkins <kyle@fizx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fizx/devvit-phaser.git"
  },
  "bugs": {
    "url": "https://github.com/fizx/devvit-phaser/issues"
  },
  "homepage": "https://fizx.github.io/devvit-phaser",
  "devDependencies": {
    "@devvit/public-api": "^0.11.7",
    "@types/uuid": "^10.0.0",
    "gh-pages": "^6.3.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "@devvit/public-api": "^0.10.0",
    "devvit": "^0.11.0",
    "phaser": "^3.60.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
